// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
using namespace std;
int main() {
    string arr[]={"Pakistan","Qatar"};
     for(int i=0;i<2;i++){
        cout<<arr[i]<<"\n";
    }
    string newa[]={"Malaysia",arr[0],arr[1]};
    for(int i=0;i<3;i++){
        cout<<newa[i]<<"\n";
    }
    return 0;
}
