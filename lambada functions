#include <iostream>
using namespace std;

void first(int val,int val2){
//[val,val2] use these to use variable as it is
//(int a, int b) use exsiting but with a different name
    auto second = [val,val2](){
        if(val == 10){
        cout<<"Second Function is running: "<<endl;}
            auto third = [val2](){
                if(val2 == 146){
                cout<<"Third Function is running: "<<endl;
                }
                else{
                    cout<<"Value is not: "<<val2<<endl;
                }
            };
            third();
    };
    second();
}

int main(){
    first(10,145);
}
