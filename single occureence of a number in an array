#include <iostream>
#include<vector>
using namespace std;

int fun(vector<int> &arr){
    int result = 0;
    for(int van : arr){
        result ^= van;
    }
    return result;
}

int main() {
   int n;
   cout<<"Enter the size of vector"<<endl;
cin>>n;
    vector<int> v(n);
    
    cout<<"Enter the values:"<<endl;
    for(int i=0; i<n; i++){
        cin>>v[i];
    }
   
  int r =  fun(v);
   cout<<r;
   
    return 0;
}
